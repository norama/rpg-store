{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@widgets/*": ["projects/rpg/builder/widgets/*"],
      "@blocks/*": ["projects/rpg/builder/blocks/*"],
      "@dashboard/*": ["projects/rpg/builder/dashboard/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "typeRoots": [
      "./node_modules/@types",
      "src/projects/rpg/@types",
      "./src/tests/@types",
      "./src/projects/rpg/@types"
    ]
  }
}
